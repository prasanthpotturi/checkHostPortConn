<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-faultResponse">
    <DisplayName>AM-faultResponse</DisplayName>
    <Properties/>
    <Set>
        <Payload contentType="application/json">
             {
           "host" : "{hostip}",
           "port": "{port}",
           "result": "AccessControlException HOST-PORT not reachable"
           }
        </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
